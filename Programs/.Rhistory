select(c(Plant.ID, ends_with("_Notes"))) %>%
pivot_longer(cols = ends_with(c("_Notes")),
names_to = c("Week_Flower"),
values_to = c("Field_Notes"),
names_pattern = "(.*)_Notes") %>%
separate_wider_position(Week_Flower, c(Week = 1, Flower = 1))
